{
  "name": "tabnine-vscode",
  "version": "3.0.3",
  "displayName": "TabNine",
  "description": "All-language autocompleter â€” TabNine uses machine learning to help you write code faster.",
  "icon": "small_logo.png",
  "author": "Jacob Jackson",
  "publisher": "TabNine",
  "repository": {
    "type": "git",
    "url": "https://github.com/codota/tabnine-vscode"
  },
  "bugs": {
    "url": "https://github.com/codota/tabnine-vscode/issues"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "autocomplete",
    "javascript",
    "typescript",
    "js",
    "python"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "tslint": "tslint -c tslint.json src/extension.ts",
    "vscode:uninstall": "node ./out/uninstall",
    "test": "tsc -p ./ && node ./out/test/runTest.js",
    "teamcity:test": "tsc -p ./ && node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^10.12.0",
    "assert": "^2.0.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "mocha-teamcity-reporter": "^3.0.0",
    "prettier": "2.1.2",
    "typescript": "^3.9.3",
    "vscode": "^1.1.21",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "await-semaphore": "^0.1.3",
    "lodash.memoize": "^4.1.2",
    "semver": "^5.1.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "TabNine::config",
        "title": "TabNine: Open Settings"
      },
      {
        "command": "TabNine::validatorModeToggle",
        "title": "TabNine: Validator toggle"
      },
      {
        "command": "TabNine::validatorClearCache",
        "title": "TabNine: Clear validator cache"
      }
    ],
    "configuration": [
      {
        "title": "TabNine",
        "properties": {
          "tabnine.disable_line_regex": {
            "type": "array",
            "default": [],
            "description": "Disable TabNine when the current line contains a match of any of the provided regexes. For example, add \"require\" to disable TabNine when the current line contains the word 'require'."
          },
          "tabnine.disable_file_regex": {
            "type": "array",
            "default": [],
            "description": "Disable TabNine when the file path contains a match of any of the provided regexes. For example, add \"[.]js$\" to disable TabNine in JavaScript files."
          },
          "tabnine.experimentalAutoImports": {
            "type": [
              "boolean",
              null,
              "number"
            ],
            "default": null,
            "description": "Experimental: Enable modules auto-import following applying TabNine suggestions"
          },
          "tabnine.logFilePath": {
            "type": "string",
            "description": "TabNine's log file (Visual Studio Code must be restarted for this setting to take effect)"
          }
        }
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+v",
        "mac": "cmd+v",
        "when": "editorFocus",
        "command": "TabNine::paste"
      },
      {
        "key": "ctrl+m",
        "mac": "cmd+m",
        "when": "editorFocus",
        "command": "TabNine::validatorModeToggle"
      }
    ]
  }
}
